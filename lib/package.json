    },
    {
      "path": "dist/msaview.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "husky": "^5.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/color": "^3.0.1",
    "@types/d3": "^6.3.0",
    "clustal-js": "^1.0.2",
    "color": "^3.1.3",
    "d3-array": "^2.12.0",
    "d3-hierarchy": "^2.0.0",
    "normalize-wheel": "^1.0.1",
    "stockholm-js": "^1.0.10"
  }
