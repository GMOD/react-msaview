{
  "name": "react-msaview",
  "author": "Colin",
  "version": "2.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "bundle",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "node output-version.js > src/version.ts && git add -A src && git commit -m '[skip ci] Bump version.ts'",
    "build:esm": "tsc",
    "build:bundle": "rollup -c --bundleConfigAsCjs",
    "build": "npm run build:esm && npm run build:bundle",
    "prepack": "npm run build",
    "postversion": "git push --follow-tags",
    "prestatedocs": "yarn tsc --build docgen/tsconfig.json",
    "statedocs": "rm -rf apidocs && mkdir apidocs && node docgen/dist/generateStateModelDocs.js"
  },
  "peerDependencies": {
    "@jbrowse/core": ">=2.0.0",
    "@mui/icons-material": ">=5.0.0",
    "@mui/material": ">=5.0.0",
    "mobx": ">=6.0.0",
    "mobx-react": ">=7.0.0",
    "mobx-state-tree": ">=5.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/color": "^3.0.1",
    "@types/d3": "^7.4.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "rollup": "^3.0.2",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "slugify": "^1.6.6",
    "tslib": "^2.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "clustal-js": "^1.0.3",
    "colord": "^2.9.3",
    "copy-to-clipboard": "^3.3.1",
    "d3-array": "^3.2.3",
    "d3-hierarchy": "^3.1.2",
    "normalize-wheel": "^1.0.1",
    "rbush": "^3.0.1",
    "stockholm-js": "^1.0.10",
    "tss-react": "^4.8.3"
  }
}
